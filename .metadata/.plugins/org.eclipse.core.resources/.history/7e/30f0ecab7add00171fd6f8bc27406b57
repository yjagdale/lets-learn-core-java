package Ashish;

public class Arrayoprations {
public static void main(String[] args) {
	
	
	max();
	
	
}

public static void max()
{
	int[] userinputs=new  int[20];
	int max=0;
	for (int i = 0; i < userinputs.length; i++) {
		userinputs[i]=i;
	}
	for (int i = 0; i < userinputs.length; i++) {
		if (userinputs[i]>max) {
			
			max=userinputs[i];
			
		}
	}
	
	for (int i = 0; i < userinputs.length; i++) {
		
		System.out.println(i+"-->"+userinputs[i]);
	}
}

}
